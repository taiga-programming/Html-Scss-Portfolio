{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Landing page for portfolio ",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taiga-programming/portfolio.git"
  },
  "author": "taiga",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/taiga-programming/portfolio/issues"
  },
  "homepage": "https://github.com/taiga-programming/portfolio#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "concat": "^1.0.3",
    "node-sass": "^4.13.0",
    "postcss-cli": "^6.1.3"
  },
  "dependencies": {}
}
